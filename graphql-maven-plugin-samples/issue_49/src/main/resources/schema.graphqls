enum Title {
  DR,
  MR,
  MS,
  MRS
}

type Account {
  id: Int!
}

input SignupForAccountInput {
  title: Title!
}

input SignupForListAccountInput {
  titles: [Title]!
}

type Mutation {
  signupForAccount(input: SignupForAccountInput!): Account!
  signupForListAccount(input: SignupForListAccountInput!): [Account]!
}

type Query {
	aQuery: String
}